# Chess AI Coach - Environment Configuration
# Copy this file to .env and fill in your values

# Path to the Stockfish binary
STOCKFISH_PATH=stockfish/stockfish-windows-x86-64-avx2.exe

# LLM Backend: "ollama" or "openai"
LLM_BACKEND=ollama

# Ollama settings (if LLM_BACKEND=ollama)
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama3

# OpenAI settings (if LLM_BACKEND=openai)
OPENAI_API_KEY=sk-your-key-here
OPENAI_MODEL=gpt-4o-mini

# Default bot difficulty (1-20, Stockfish Skill Level)
BOT_SKILL_LEVEL=5

# Player approximate ELO (used for coaching context)
PLAYER_ELO=1000
